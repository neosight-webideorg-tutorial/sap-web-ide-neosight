<mvc:View controllerName="demo.neosight.restapi.RESTAPI.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<List id="contactList" width="auto" items="{items>/}">
						<ObjectListItem title="{items>first_name}"/>
					</List>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>